{
  "name": "PasswordManagerTeleBot",
  "description": "Telegram Bot for Password Management",
  "repository": "https://github.com/GopalSaraf/PasswordManagerTeleBot",
  "logo": "https://telegra.ph/file/25c3e3246dee8df0cc91c.jpg",
  "keywords": [
    "Telegram",
    "Password"
  ],
  "env": {
    "ENV": {
      "description": "Set it to anything to enable configuration by environment variables.",
      "value": "ANYTHING"
    },
    "BOT_USERNAME": {
      "descreption": "Bot username with @. like @pwdmanagerbot"
    },
    "BOT_TOKEN": {
      "description": "Get it from @BotFather"
    },
    "APP_ID": {
      "description": "Your API_ID, Get it from https://my.telegram.org/apps"
    },
    "API_HASH": {
      "description": "Your API_HASH, Get it from https://my.telegram.org/apps"
    },
    "DATABASE_URL": {
      "descreption": "Your heroku database url here."
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ]
}
